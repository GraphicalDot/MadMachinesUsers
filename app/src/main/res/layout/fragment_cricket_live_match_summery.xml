<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:focusableInTouchMode="true"
    tools:context="com.sports.unity.common.controller.UserProfileActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/horizontal_padding_very_small"
        android:layout_marginBottom="@dimen/horizontal_padding_small"
        android:layout_marginLeft="@dimen/horizontal_padding_small"
        android:layout_marginRight="@dimen/horizontal_padding_small"
        android:background="@color/ColorPrimary"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_last_six_ball"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_vertical_margin_small"
                android:layout_marginTop="@dimen/vertical_padding_very_very_small"
                android:textColor="@color/dark_blue"
                android:textSize="@dimen/font_size_medium"
                android:text="RECENT"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray3" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="6"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/tv_first_ball"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/ic_distance_slider_01"/>
            <ImageView
                android:id="@+id/tv_second_ball"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/ic_distance_slider_01"/>
            <ImageView
                android:id="@+id/tv_third_ball"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/ic_distance_slider_01"/>
            <ImageView
                android:id="@+id/tv_fourth_ball"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/ic_distance_slider_01"/>
            <ImageView
                android:id="@+id/tv_fifth_ball"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/ic_distance_slider_01"/>
            <ImageView
                android:id="@+id/tv_six_ball"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:src="@drawable/ic_distance_slider_01"/>
        </LinearLayout>

    </LinearLayout>
    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/horizontal_padding_very_small"
    android:layout_marginBottom="@dimen/horizontal_padding_small"
    android:layout_marginLeft="@dimen/horizontal_padding_small"
    android:layout_marginRight="@dimen/horizontal_padding_small"
    android:background="@color/ColorPrimary"
    android:orientation="vertical">
        <LinearLayout
            android:id="@+id/lo_batting"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_batting"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_vertical_margin_small"
                android:layout_marginTop="@dimen/vertical_padding_very_very_small"
                android:textColor="@color/dark_blue"
                android:textSize="@dimen/font_size_medium"
                android:text="BATTING"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray3" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/ColorPrimary"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/lo_match_summery"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="@dimen/vertical_padding_very_very_small"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/iv_player_first"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="@dimen/activity_vertical_margin_small"
                    android:src="@drawable/ic_user"/>
                <View
                    android:id="@+id/v_divider"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="@dimen/margin_divider"
                    android:layout_alignLeft="@+id/iv_player_first"
                    android:layout_centerInParent="true"
                    android:layout_alignRight="@+id/iv_player_second"
                    android:layout_marginRight="@dimen/margin_divider"
                    android:background="@color/gray3" />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/margin_divider"
                    android:layout_alignParentRight="true"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tv_first_player_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:textSize="@dimen/font_size_small_large"
                        android:text="M.Marsh"
                        />
                    <TextView
                        android:id="@+id/tv_first_player_run_rate"
                        android:layout_width="wrap_content"
                        android:layout_alignTop="@+id/v_divider"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:textColor="@color/score_light_grey"
                        android:textSize="@dimen/font_size_very_small"
                        android:text="SR 85.71"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/margin_divider"
                    android:layout_alignParentRight="true"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tv_second_player_name"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:textSize="@dimen/font_size_small_large"
                        android:text="M.Wade"
                        />
                    <TextView
                        android:id="@+id/tv_second_player_run_rate"
                        android:layout_width="wrap_content"
                        android:layout_alignTop="@+id/v_divider"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:textColor="@color/score_light_grey"
                        android:textSize="@dimen/font_size_very_small"
                        android:text="SR 60.00"
                        />
                </LinearLayout>

                <ImageView
                    android:id="@+id/iv_player_second"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/activity_vertical_margin_small"
                    android:src="@drawable/ic_user"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/margin_divider"
                    android:layout_marginRight="@dimen/margin_divider"
                    android:layout_alignBottom="@+id/iv_player_first">


                    <TextView
                        android:id="@+id/tv_first_player_run_on_ball"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/font_size_very_small"
                        android:text="6(7)"
                        android:gravity="left"
                        android:layout_weight="1"
                        />
                    <TextView
                        android:id="@+id/tv_partnership_record"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:textSize="@dimen/font_size_very_small"
                        android:text="10(12)RRS"
                        android:gravity="center"
                        />
                    <TextView
                        android:id="@+id/tv_second_player_run_on_ball"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/font_size_very_small"
                        android:text="3(5)"
                        android:layout_weight="1"
                        android:gravity="right"
                        />
                </LinearLayout>

            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/horizontal_padding_very_small"
        android:layout_marginBottom="@dimen/horizontal_padding_small"
        android:layout_marginLeft="@dimen/horizontal_padding_small"
        android:layout_marginRight="@dimen/horizontal_padding_small"
        android:background="@color/ColorPrimary"
        android:orientation="vertical">
        <LinearLayout
            android:id="@+id/lo_child_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_upcoming"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_vertical_margin_small"
                android:layout_marginTop="@dimen/vertical_padding_very_very_small"
                android:textColor="@color/dark_blue"
                android:textSize="@dimen/font_size_medium"
                android:text="UPCOMING"
                />
        </LinearLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/gray3" />
        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/vertical_padding_very_very_small">
            <TableRow>
                <ImageView
                    android:id="@+id/iv_upp_coming_player_first"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/activity_vertical_margin_small"
                    android:src="@drawable/ic_user"/>

                <ImageView
                    android:id="@+id/iv_up_coming_player_second"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:src="@drawable/ic_user"/>
                <ImageView
                    android:id="@+id/iv_up_coming_player_third"
                    android:layout_width="wrap_content"
                    android:layout_marginRight="@dimen/activity_vertical_margin_small"
                    android:src="@drawable/ic_user"/>
            </TableRow>
            <TableRow>
                <TextView
                    android:id="@+id/tv_first_up_coming_player_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginLeft="@dimen/activity_vertical_margin_small"

                    android:textSize="@dimen/font_size_small_large"
                    android:text="J.Faulkner"/>
                <TextView
                    android:id="@+id/tv_second_up_coming_player_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:textSize="@dimen/font_size_small_large"
                    android:text="P.Hasting"/>
                <TextView
                    android:id="@+id/tv_third_up_coming_player_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:layout_marginRight="@dimen/activity_vertical_margin_small"
                    android:textSize="@dimen/font_size_small_large"
                    android:text="K.Richardson"/>
            </TableRow>
            <TableRow>
                <TextView
                    android:id="@+id/tv_first_up_coming_player_run_rate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/score_light_grey"
                    android:textSize="@dimen/font_size_very_small"
                    android:layout_marginLeft="@dimen/activity_vertical_margin_small"

                    android:text="SR.109.09"/>
                <TextView
                    android:id="@+id/tv_second_up_coming_player_run_rate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/score_light_grey"
                    android:textSize="@dimen/font_size_very_small"
                    android:layout_weight="1"
                    android:text="SR 88.89"/>
                <TextView
                    android:id="@+id/tv_third_up_coming_player_run_rate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/score_light_grey"
                    android:textSize="@dimen/font_size_very_small"
                    android:layout_marginRight="@dimen/activity_vertical_margin_small"
                    android:text="SR 146.15"/>
            </TableRow>

        </TableLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/horizontal_padding_very_small"
        android:layout_marginBottom="@dimen/horizontal_padding_small"
        android:layout_marginLeft="@dimen/horizontal_padding_small"
        android:layout_marginRight="@dimen/horizontal_padding_small"
        android:background="@color/ColorPrimary"
        android:orientation="vertical">
        <LinearLayout
            android:id="@+id/lo_bowling"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_bowling"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/activity_vertical_margin_small"
                android:layout_marginTop="@dimen/vertical_padding_very_very_small"
                android:textColor="@color/dark_blue"
                android:textSize="@dimen/font_size_medium"
                android:text="BOWLING"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="@color/gray3" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/ColorPrimary"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/iv_upp_coming_bowler"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/activity_vertical_margin_small"
                    android:layout_marginLeft="@dimen/activity_vertical_margin_small"
                    android:src="@drawable/ic_user"/>
                <TableLayout
                    android:layout_width="fill_parent"
                    android:layout_marginTop="@dimen/activity_vertical_margin_small"
                    android:layout_height="wrap_content">
                    <TableRow>
                        <TextView
                            android:id="@+id/tv_bowler_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:textSize="@dimen/font_size_small_large"
                            android:text="C.Anderson"/>
                        <TextView
                            android:id="@+id/tv_bowler_over"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:textSize="@dimen/font_size_small_large"
                            android:text="1.0"/>
                        <TextView
                            android:id="@+id/tv_bowler_W_Run"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:textSize="@dimen/font_size_small_large"
                            android:text="0/4"/>
                    </TableRow>
                    <TableRow>
                        <TextView
                            android:id="@+id/tv_bowler_econ"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:textColor="@color/score_light_grey"
                            android:textSize="@dimen/font_size_very_small"
                            android:text="ECON 4.0"/>
                        <TextView
                            android:id="@+id/tv_bowler_overs"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:textColor="@color/score_light_grey"
                            android:textSize="@dimen/font_size_very_small"
                            android:text="OVER"/>
                        <TextView
                            android:id="@+id/tv_bowler_wr"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:textColor="@color/score_light_grey"
                            android:textSize="@dimen/font_size_very_small"
                            android:text="W/R"/>
                    </TableRow>
                </TableLayout>
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>



</LinearLayout>
